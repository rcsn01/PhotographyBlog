{
  "homepage": "https://rcsn01.github.io/PhotographyBlog",
  "name": "my-nextjs-blog",
  "version": "1.0.0",
  "description": "A simple blog built with Next.js and Markdown posts",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:rcsn01/PhotographyBlog.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "mkdir -p out && touch out/.nojekyll",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d out -b gh-pages -r git@github.com:rcsn01/PhotographyBlog.git -m 'Deploy $(date +\"%Y-%m-%d %H:%M:%S\")'",
    "start": "next start",
    "generate-posts": "node generatePosts.js"
  },
  "dependencies": {
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "grey-matter": "^0.0.0",
    "gsap": "^3.13.0",
    "next": "latest",
    "ogl": "^1.0.11",
    "react": "latest",
    "react-dom": "latest",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "remark": "latest",
    "remark-html": "latest",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "gh-pages": "^6.3.0",
    "typescript": "latest"
  },
  "license": "MIT"
}
